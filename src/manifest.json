{
  "manifest_version": 3,
  "name": "Multilingual Voice Search",
  "version": "1.2.1",
  "description": "Google voice search in languages different from your OS and location settings.",
  "permissions": ["storage"],
  "icons": {
    "16": "vs_icon.png",
    "32": "vs_icon.png",
    "48": "vs_icon.png",
    "128": "vs_icon.png"
  },
  "action": {
    "default_icon": {
      "16": "vs_icon.png",
      "32": "vs_icon.png",
      "48": "vs_icon.png",
      "128": "vs_icon.png"
    }
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}
